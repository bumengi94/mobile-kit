{
	"presets": ["module:metro-react-native-babel-preset"],
	"plugins": [
		"react-native-reanimated/plugin",
		[
			"module-resolver",
			{
				"alias": {
					"#components": "./src/components",
					"#languages": "./src/languages",
					"#redux": "./src/redux",
					"#routers": "./src/routers",
					"#utils": "./src/utils"
				}
			}
		]
	],
	"env": {
		"production": {
			"plugins": ["react-native-paper/babel"]
		}
	}
}
